
{
  "redirects": [
    {
      "source": "https://www.moneybharat.co/(.*)",
      "destination": "https://moneybharat.co/$1",
      "permanent": true
    },
    {
      "source": "http://www.moneybharat.co/(.*)",
      "destination": "https://moneybharat.co/$1",
      "permanent": true
    },
    {
      "source": "http://moneybharat.co/(.*)",
      "destination": "https://moneybharat.co/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
